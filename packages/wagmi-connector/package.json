{
  "name": "@talismn/wagmi-connector",
  "version": "0.3.0",
  "license": "GPL-3.0-or-later",
  "author": "TalismanSociety",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "Talisman",
    "wagmi",
    "connector"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist && rimraf node_modules && rimraf .turbo",
    "publish": "npm publish"
  },
  "peerDependencies": {
    "@wagmi/core": "1.x"
  },
  "peerDependenciesMeta": {
    "@wagmi/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@wagmi/core": "^1.0.5",
    "tsup": "^6.7.0",
    "typescript": "^4.9.4",
    "viem": "^0.3.31"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "platform": "browser",
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ]
  }
}
